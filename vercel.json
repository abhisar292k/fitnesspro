{
    "version": 2,
    "builds": [
        {
            "src": "fitnessclubbyabhi/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "100mb",
                "runtime": "python3.9"
            },
            "buildCommand": "python manage.py collectstatic --noinput"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "fitnessclubbyabhi/wsgi.py"
        }
    ]
}
